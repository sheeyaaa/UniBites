<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- White background -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" />
            <corners android:radius="4dp" />
            <padding android:top="8dp" android:left="8dp" android:right="8dp" android:bottom="8dp" />
        </shape>
    </item>
    
    <!-- Outer square -->
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="2dp" android:color="#000000" />
            <solid android:color="#FFFFFF" />
            <corners android:radius="2dp" />
        </shape>
    </item>
    
    <!-- Inner square pattern 1 - top left -->
    <item android:left="16dp" android:top="16dp" android:right="140dp" android:bottom="140dp">
        <shape android:shape="rectangle">
            <solid android:color="#000000" />
        </shape>
    </item>
    
    <!-- Inner square pattern 2 - top right -->
    <item android:left="140dp" android:top="16dp" android:right="16dp" android:bottom="140dp">
        <shape android:shape="rectangle">
            <solid android:color="#000000" />
        </shape>
    </item>
    
    <!-- Inner square pattern 3 - bottom left -->
    <item android:left="16dp" android:top="140dp" android:right="140dp" android:bottom="16dp">
        <shape android:shape="rectangle">
            <solid android:color="#000000" />
        </shape>
    </item>
    
    <!-- Center logo -->
    <item android:left="95dp" android:top="95dp" android:right="95dp" android:bottom="95dp">
        <shape android:shape="oval">
            <solid android:color="#FF6D3A" />
        </shape>
    </item>
    
    <!-- Additional QR code patterns -->
    <item android:left="60dp" android:top="60dp" android:right="120dp" android:bottom="120dp">
        <shape android:shape="rectangle">
            <solid android:color="#000000" />
        </shape>
    </item>
    
    <item android:left="120dp" android:top="60dp" android:right="60dp" android:bottom="120dp">
        <shape android:shape="rectangle">
            <solid android:color="#000000" />
        </shape>
    </item>
    
    <item android:left="60dp" android:top="120dp" android:right="120dp" android:bottom="60dp">
        <shape android:shape="rectangle">
            <solid android:color="#000000" />
        </shape>
    </item>
    
    <item android:left="120dp" android:top="120dp" android:right="60dp" android:bottom="60dp">
        <shape android:shape="rectangle">
            <solid android:color="#000000" />
        </shape>
    </item>
</layer-list> 